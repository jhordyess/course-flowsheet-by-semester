{
  "name": "course-flowsheet-by-semester",
  "description": "This project represents one of my first steps in programming, approximately in 2017.",
  "version": "1.1.0",
  "author": "Jhordy Gavinchu <dev@jhordyess.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "react",
    "typescript",
    "university"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
